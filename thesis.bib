@article{lee_characterization_2022,
	title = {Characterization of the {Core} {Determinants} of {Social} {Influence} {From} a {Computational} and {Cognitive} {Perspective}},
	volume = {13},
	issn = {1664-0640},
	url = {https://www.frontiersin.org/journals/psychiatry/articles/10.3389/fpsyt.2022.846535/full},
	doi = {10.3389/fpsyt.2022.846535},
	abstract = {{\textless}p{\textgreater}Most human decisions are made among social others, and in what social context the choices are made is known to influence individuals' decisions. Social influence has been noted as an important factor that may nudge individuals to take more risks (e.g., initiation of substance use), but ironically also help individuals to take safer actions (e.g., successful abstinence). Such bi-directional impacts of social influence hint at the complexity of social information processing. Here, we first review the recent computational approaches that shed light on neural and behavioral mechanisms underlying social influence following basic computations involved in decision-making: valuation, action selection, and learning. We next review the studies on social influence from various fields including neuroeconomics, developmental psychology, social psychology, and cognitive neuroscience, and highlight three dimensions of determinants—who are the recipients, how the social contexts are presented, and to what domains and processes of decisions the influence is applied—that modulate the extent to which individuals are influenced by others. Throughout the review, we also introduce the brain regions that were suggested as neural instantiations of social influence from a large body of functional neuroimaging studies. Finally, we outline the remaining questions to be addressed in the translational application of computational and cognitive theories of social influence to psychopathology and health.{\textless}/p{\textgreater}},
	language = {English},
	urldate = {2024-05-15},
	journal = {Frontiers in Psychiatry},
	author = {Lee, Hyeji and Chung, Dongil},
	month = apr,
	year = {2022},
	note = {Publisher: Frontiers},
	keywords = {computational modeling, Computational Psychiatry, context dependence, individual differences, social influence},
	file = {Full Text:/Users/aamirsohail/Zotero/storage/36ZAYQU3/Lee and Chung - 2022 - Characterization of the Core Determinants of Socia.pdf:application/pdf},
}

@article{rouault_psychiatric_2018,
	series = {Translating {Biology} to {Treatment} in {Schizophrenia}},
	title = {Psychiatric {Symptom} {Dimensions} {Are} {Associated} {With} {Dissociable} {Shifts} in {Metacognition} but {Not} {Task} {Performance}},
	volume = {84},
	issn = {0006-3223},
	url = {https://www.sciencedirect.com/science/article/pii/S0006322318300295},
	doi = {10.1016/j.biopsych.2017.12.017},
	abstract = {Background
Distortions in metacognition—the ability to reflect on and control other cognitive processes—are thought to be characteristic of poor mental health. However, it remains unknown whether such shifts in self-evaluation are due to specific alterations in metacognition and/or a downstream consequence of changes in decision-making processes.
Methods
Using perceptual decision making as a model system, we employed a computational psychiatry approach to relate parameters governing both decision formation and metacognitive evaluation to self-reported transdiagnostic symptom dimensions in a large general population sample (N = 995).
Results
Variability in psychopathology was unrelated to either speed or accuracy of decision formation. In contrast, leveraging a dimensional approach, we revealed independent relationships between psychopathology and metacognition: a symptom dimension related to anxiety and depression was associated with lower confidence and heightened metacognitive efficiency, whereas a dimension characterizing compulsive behavior and intrusive thoughts was associated with higher confidence and lower metacognitive efficiency. Furthermore, we obtained a robust double dissociation—whereas psychiatric symptoms predicted changes in metacognition but not decision performance, age predicted changes in decision performance but not metacognition.
Conclusions
Our findings indicate a specific and pervasive link between metacognition and mental health. Our study bridges a gap between an emerging neuroscience of decision making and an understanding of metacognitive alterations in psychopathology.},
	number = {6},
	urldate = {2024-04-22},
	journal = {Biological Psychiatry},
	author = {Rouault, Marion and Seow, Tricia and Gillan, Claire M. and Fleming, Stephen M.},
	month = sep,
	year = {2018},
	keywords = {Cognitive neuroscience, Decision making, Computational psychiatry, Psychopathology, Confidence, Metacognition},
	pages = {443--451},
	file = {Full Text:/Users/aamirsohail/Zotero/storage/S34V78ME/Rouault et al. - 2018 - Psychiatric Symptom Dimensions Are Associated With.pdf:application/pdf},
}

@article{seow_transdiagnostic_2020,
	title = {Transdiagnostic {Phenotyping} {Reveals} a {Host} of {Metacognitive} {Deficits} {Implicated} in {Compulsivity}},
	volume = {10},
