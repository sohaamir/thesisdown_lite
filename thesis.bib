	note = {Publisher: Public Library of Science},
	keywords = {Behavior, Decision making, Games, Experimental design, Thalamus, Parietal lobe, Delta functions, Social influence},
	pages = {e52630},
	file = {Full Text PDF:/Users/aamirsohail/Zotero/storage/88CPMLPK/Tomlin et al. - 2013 - The Neural Substrates of Social Influence on Decis.pdf:application/pdf},
}

